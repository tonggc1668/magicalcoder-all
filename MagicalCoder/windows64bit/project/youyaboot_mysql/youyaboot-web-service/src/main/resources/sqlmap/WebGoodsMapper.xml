<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
这里是我自定义新查询的地方
-->

<mapper namespace="com.magicalcoder.youyaboot.web.webservice.goods.mapper.WebGoodsMapper">
    <!-- 查询单个实体 -->
    <select id="getWebGoods" parameterType="hashmap" resultType="WebGoods">
        select   id
        from goods limit 1
    </select>


</mapper>
